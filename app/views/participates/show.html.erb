<div id="content_detail" class="ui-corner-all">
<h1 class="title"><%= t('page.showing', :model => t('activerecord.models.participate')) -%></h1>
<div id="content_list">
<p id="notice"><%= notice %></p>

<p>
  <strong><%= t('activerecord.models.patron') -%>:</strong>
  <%= link_to @participate.patron.full_name, @participate.patron if @participate.patron %>
</p>

<p>
  <strong><%= t('activerecord.models.event') -%>:</strong>
  <%= link_to @participate.event.display_name.localize, @participate.event if @participate.event %>
</p>

</div>
</div>

<div id="submenu" class="ui-corner-all">
  <ul>
    <li><%= link_to t('page.edit'), edit_participate_path(@participate) %></li>
    <li><%= back_to_index(flash[:page_info]) %></li>
  </ul>
</div>
